<html><h1>Listing Class Discussion Scores</h1>

<table>
  <tr>
    <th>Student</th>
    <th>Score</th>
    <th><%= link_to 'Week1', "/discs/editRow/week1" %></th>
    <th><%= link_to 'Week2', "/discs/editRow/week2" %></th>
    <th><%= link_to 'Week3', "/discs/editRow/week3" %></th>
    <th><%= link_to 'Week4', "/discs/editRow/week4" %></th>
    <th><%= link_to 'Week5', "/discs/editRow/week5" %></th>
    <th><%= link_to 'Week6', "/discs/editRow/week6" %></th>
    <th><%= link_to 'Week7', "/discs/editRow/week7" %></th>
    <th><%= link_to 'Week8', "/discs/editRow/week8" %></th>
    <th><%= link_to 'Week9', "/discs/editRow/week9" %></th>
    <th><%= link_to 'Week10', "/discs/editRow/week10" %></th>
    <th><%= link_to 'Week11', "/discs/editRow/week11" %></th>

    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @discs.each do |disc| %>
  <tr>
    <td><%= Student.find(disc.student_id).name %></td>
    <td><%= disc.score %></td>
    <td><%= disc.week1 %></td>
    <td><%= disc.week2 %></td>
    <td><%= disc.week3 %></td>
    <td><%= disc.week4 %></td>
    <td><%= disc.week5 %></td>
    <td><%= disc.week6 %></td>
    <td><%= disc.week7 %></td>
    <td><%= disc.week8 %></td>
    <td><%= disc.week9 %></td>
    <td><%= disc.week10 %></td>
    <td><%= disc.week11 %></td>
    <td><%= link_to 'Edit', edit_disc_path(disc) %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'Frontpage', students_path %>
</html>