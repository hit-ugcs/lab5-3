<h1>Listing finals</h1>

<table>
  <tr>
    <th>Score</th>
    <th>Student</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @finals.each do |final| %>
  <tr>
    <td><%= final.score %></td>
    <td><%= final.student_id %></td>
    <td><%= link_to 'Show', final %></td>
    <td><%= link_to 'Edit', edit_final_path(final) %></td>
    <td><%= link_to 'Destroy', final, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Final', new_final_path %>
